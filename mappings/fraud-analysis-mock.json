{
  "request": {
    "method": "POST",
    "url": "/mock/fraud-analysis",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.insuranceId)]" },
      { "matchesJsonPath": "$[?(@.customerId)]" }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "insuranceId": "{{jsonPath request.body '$.insuranceId'}}",
      "customerId": "{{jsonPath request.body '$.customerId'}}",
      "customerType": "REGULAR",
      "analyzedAt": "2024-06-10T15:30:00Z",
      "findings": ["SUSPICIOUS_ACTIVITY"]
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"]
  }
}
